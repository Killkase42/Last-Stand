import pygame

Black = (0, 0, 0)
White = (255, 255, 255)
Green = (0, 255, 0)
Red = (255, 0, 0)

size = (700, 500)
Screen = pygame.display.set_mode(size)

pygame.display.set_caption(“Last Stand”)

done=False

clock=pygame.time.Clock()

while not done:
	For event in pygame.event.get():
        if event.type == pygame.QUIT:
            done = True
